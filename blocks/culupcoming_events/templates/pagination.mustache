{{!
    @template block_culupcoming_events/pagination

    CUL Upcoming Events pagination view.

    The purpose of this template is to render the CUL Upcoming Events pagination.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Example context (json):
    {
    }
}}

{{#pagination}}
<div class="pages">
    {{#prev}}
        <a href="{{{prevurl}}}">
            {{prevtext}}
        </a>
    {{/prev}}
    {{{sep}}}
    {{#next}}
        <a href="{{{nexturl}}}">
            {{nexttext}}
        </a>
    {{/next}}
</div>
{{/pagination}}